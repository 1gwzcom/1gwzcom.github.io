    //候选词
    var bind_name = 'input';
    if (navigator.userAgent.indexOf("MSIE") != -1){
        bind_name = 'propertychange';
    }


    function searchAjax(scope){
        var _this = scope.parent().parent();
        var _cur =scope;
        var landIndex = _this.find('.search-land li.active').length > 0 ? _this.find('.search-land li.active').index() : 0;

        if(event.keyCode == 40){
            landIndex = _this.find('.search-land li.active').length ? landIndex + 1 : 0;
            landIndex = landIndex > _this.find('.search-land li').length-1 ? 0 : landIndex;
            _this.find('.search-land li').removeClass('active');
            _this.find('.search-land').find("li:eq("+(landIndex)+")").addClass("active");
            scope.val(_this.find('.search-land li.active').html());
            return false;
        }
        if(event.keyCode == 38){
            landIndex =  _this.find('.search-land li.active').length ? landIndex - 1 : _this.find('.search-land li').length-1;
            landIndex = landIndex < 0 ? _this.find('.search-land li').length-1 : landIndex;
            _this.find('.search-land li').removeClass('active');
            _this.find('.search-land').find("li:eq("+(landIndex)+")").addClass("active");
            scope.val(_this.find('.search-land li.active').html());
            return false;
        }

        const _sl = scope.parents('form').siblings('.search-land');
        const _value = scope.val();
        var content = "";
        content = _value.replace(/\s+/g, "");
        if(content.length == 0){
            $('.src-close-btn').hide();
            _sl.hide();
        }else{
            $('.src-close-btn').show();
        }

        var _url = "/index.php?r=index/kwarr";

        $.ajax({
            url: _url,
            type: 'get',
            dataType: 'json',
            data: {
                kw: _value
            },
            success: function (data) {
                if (data.status == '1') {
                    if(data.data.length == 0 ){
                    }else{
                        _cur.parent('form').siblings('.search-land').show();
                        _cur.parent('form').siblings('.search-land').find('li').each(function(i){
                            if(i>data.data.length-1){
                                _cur.parent('form').siblings('.search-land').find('li').eq(i).hide();
                            }else{
                                if(scope.val()==''){
                                }else{
                                    _cur.parent('form').siblings('.search-land').find('li').eq(i).show().text(data.data[i]);
                                }
                            }
                        });
                    }

                }else{
                }
                const _value = _cur.val();
                var content = "";
                content = _value.replace(/\s+/g, "");
                if(content.length == 0){
                    $('.src-close-btn').hide();
                    _sl.hide();
                }else{
                    $('.src-close-btn').show();
                }
            }
        })
    }

    //搜索框点击键盘 事件
    $(".my-src-input").focus(function(){
        jQuery.fn.isChildAndSelfOf = function(b){
            return (this.closest(b).length > 0);
        };
        var _this = $(this).parent().parent();
        $("body").unbind("click").on("click",function(event){
            if(!$(event.target).isChildAndSelfOf(_this)){
                $(".search-land").hide();
            }
        });
        searchAjax($(this));
    }).keyup(function(event){
        searchAjax($(this));
        if($(this).val()==''){
            $(this).parent('form').siblings('.search-land').find('li').text('');
        }
    });

    $(".search-land").on('click','li',function(){
        $(".my-src-input").val($(this).text());
        $('.my-src-btn').trigger('click');
    });
    $(".search-land").on('click','p',function(){
        $(".search-land").hide();
    });

    $('.src-close-btn').click(function(){
        $('.src-close-btn').hide();
        $('.search-land').hide();
        if($(".my-src-input").attr('value')==''){
            $(".my-src-input").val('');
        }else{
            $(".my-src-input").val('').attr('placeholder','请输入要搜索的词');
        }

    });

    var kw_url = "/index.php?r=l&kw=%E7%A7%8B%E8%A3%85";

    $('#search button').on('click',function(){
        var _this = $(this);
        if($(this).siblings('input[name="kw"]').val()==''&& kw_url != ""){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    })

    $('.floatNav .srcBtn').on('click',function(){
        var _this = $(this);
        if($('input[name="kw"]').val()==''&& kw_url != "" ){
            window.location.href=$("#m_url").val();
            return false;
        }else{
            _this.parents('form').attr('action',$("#h_url").val())
        }
    });
