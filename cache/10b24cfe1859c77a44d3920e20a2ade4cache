<script>
    $('.fb-error-type li').click(function(){
        if(!$(this).hasClass('cur')){
            $('.fb-error-type li').removeClass('cur').css('border','1px solid #ddd');
            $(this).addClass('cur').css('border','1px solid transparent');
        }
    });

    $('.fb-error-type li').mouseover(function(){
        $(this).css('border','1px solid transparent');
    })
    $('.fb-error-type li').mouseleave(function(){
        $(this).css('border','1px solid #ddd');
    })
    $('.fb-error-type li').eq(0).addClass('cur');


    if($(".fb-pc-text").val()==''){
        $('.fb-sub').attr('disabled','disabled').addClass('disabled')
    }else{
        $('.fb-sub').removeAttr('disabled').removeClass('disabled')
    }

    $(".fb-pc-text").keyup(function(){
        if($(".fb-pc-text").val()==''){
            $('.fb-sub').attr('disabled','disabled').addClass('disabled')
        }else{
            $('.fb-sub').removeAttr('disabled').removeClass('disabled')
        }

    });
    $('.fb-sub').on('click',function(){
        $('.fb-sub').attr('disabled','disabled').addClass('disabled');

        $.ajax({
            url:"/index.php?r=index/feedbacksave",
            data:{
                'type':-1,
                'source_type':1,
                'content':$('textarea[name="content"]').val(),
                'gid':$('.feed-back-gid').val(),
                'err_type':$('.fb-error-type li.cur').attr('value')
            },
            type:'post',
            dataType:'json',
            success:function(data){
                if(data.status==1){
                    layer.msg("<p>感谢您的反馈！<br>我们会尽快进行处理</p>");
                    document.getElementById("myform").reset();
                    setTimeout(function(){
                        window.location.href="/index.php?r=l/d&id=16328046";
                    },1000);
                }else{
                    $('.fb-sub').removeAttr('disabled').removeClass('disabled')
                }

            }
        });
    });
</script></div>

